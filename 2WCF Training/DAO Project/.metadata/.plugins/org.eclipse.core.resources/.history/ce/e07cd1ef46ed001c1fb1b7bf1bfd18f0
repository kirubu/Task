package org.gorl.bean;

import java.util.Set;

public class Person 
{
	private int p_id;
	private String p_name;
	private int p_age;
	private Set order;
	
	public Person(int p_id, String p_name, int p_age) 
	{
		super();
		this.p_id = p_id;
		this.p_name = p_name;
		this.p_age = p_age;
	}
	
	public Person() {	}

	public int getP_id() {
		return p_id;
	}

	public void setP_id(int p_id) {
		this.p_id = p_id;
	}

	public String getP_name() {
		return p_name;
	}

	public void setP_name(String p_name) {
		this.p_name = p_name;
	}

	public int getP_age() {
		return p_age;
	}

	public void setP_age(int p_age) {
		this.p_age = p_age;
	}

	public Set getOrder() {
		return order;
	}

	public void setOrder(Set order) {
		this.order = order;
	}

	@Override
	public String toString() {
		return "Person [p_id=" + p_id + ", p_name=" + p_name + ", p_age=" + p_age + "]";
	}
	
	
	
}
